@{
}
<table class="table">
    <tr>
        <th>Booking</th>
        <th>Amount</th>
        <th>UTR</th>
        <th>Action</th>
    </tr>

    @foreach (var p in Model)
    {
        <tr>
            <td>@p.CabBooking.BookingNumber</td>
            <td>₹@p.Amount</td>
            <td>@p.UTR</td>
            <td>
                <a asp-action="ApprovePayment" asp-route-id="@p.PaymentId"
                   class="btn btn-success">Approve</a>
            </td>
        </tr>
    }
</table>
